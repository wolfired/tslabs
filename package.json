  },
  "homepage": "https://github.com/wolfired/tslabs#readme",
  "dependencies": {
    "@types/ws": "^3.2.1",
    "ws": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^8.0.53"
  }
}
