{
  "name": "tslabs",
  "version": "0.0.1",
  "description": "Ts实验室",
  "scripts": {
    "bin": "tsc -p . --outDir ./bin --preserveConstEnums --sourceMap",
    "exp": "tsc -p . --outDir ./lib --declaration",
    "lib": "tsc -p . --outDir ./lib --declaration --noEmitOnError --noFallthroughCasesInSwitch --noUnusedLocals --noUnusedParameters --removeComments",
    "clean": "rm -rf bin/ lib/",
    "deps": "rm -rf node_modules/ && npm install",
    "types": "rm -rf typings/ && typings install",
    "prebuild": "npm run types && slt_exp src",
    "build": "npm run lib 1> /dev/null 2>&1 || npm run exp",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wolfired/tslabs.git"
  },
  "author": "LinkWu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wolfired/tslabs/issues"
  },
  "homepage": "https://github.com/wolfired/tslabs#readme",
  "dependencies": {
    "gots": "github:wolfired/gots#develop"
  }
}
